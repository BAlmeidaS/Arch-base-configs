{
  "load_extensions": {
    "nbextensions_configurator/config_menu/main": true,
    "contrib_nbextensions_help_item/main": true,
    "codefolding/main": true,
    "hinterland/hinterland": false,
    "freeze/main": true,
    "runtools/main": true,
    "collapsible_headings/main": true,
    "splitcell/splitcell": true,
    "toc2/main": false,
    "spellchecker/main": true,
    "toc": false,
    "code_prettify/autopep8": true
  },
  "hinterland": {
    "hint_delay": "400"
  },
  "Freeze": {
    "frozen_color": "#ffdddf"
  },
  "keys": {
    "command": {
      "bind": {
        "alt-b": "jupyter-notebook:run-all-cells-below",
        "9,9": "jupyter-notebook:confirm-restart-kernel-and-run-all-cells",
        "0,0": "jupyter-notebook:confirm-restart-kernel-and-clear-output"
      },
      "unbind": [
        "0,0"
      ]
    }
  },
  "autopep8": {
    "hotkeys": {
      "process_selected": "shift-p"
    }
  }
}